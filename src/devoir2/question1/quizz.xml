<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="quizWithConstructor" class="devoir2.question1.QuizzMasterService">
    <constructor-arg ref="cmmQuiz"/>
  </bean>
  <bean id="quizWithSetter" class="devoir2.question1.QuizzMasterService" lazy-init="true">
    <property name="quizMaster" ref="cmmQuiz"/>
  </bean>
  <bean id="cmmQuiz" class="devoir2.question1.CMMQuiz"/>
  <bean id="umlQuiz" class="devoir2.question1.UMLQuiz"/>
</beans>